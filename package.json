{
  "name": "cytoscape",
  "version": "3.3.0-unstable",
  "license": "MIT",
  "description": "Graph theory (a.k.a. network) library for analysis and visualisation",
  "homepage": "http://js.cytoscape.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/cytoscape/cytoscape.js.git"
  },
  "bugs": {
    "url": "https://github.com/cytoscape/cytoscape.js/issues"
  },
  "author": {
    "name": "Max Franz",
    "email": "maxkfranz@gmail.com",
    "url": "http://maxfranz.com"
  },
  "contributors": [
    {
      "name": "Max Franz",
      "email": "maxkfranz@gmail.com",
      "url": "http://maxfranz.com"
    },
    {
      "name": "Christian Lopes",
      "email": "chrtannus@gmail.com"
    },
    {
      "name": "Yue Dong",
      "email": "oryato@gmail.com"
    },
    {
      "name": "Onur Sumer",
      "email": "s.onur.sumer@gmail.com"
    },
    {
      "name": "Gerardo Huck",
      "email": "gerardohuck@gmail.com"
    },
    {
      "name": "Dylan Fong",
      "email": "dylanfong.ut@gmail.com"
    },
    {
      "name": "Joseph Stahl",
      "email": "joseph@josephstahl.com"
    }
  ],
  "keywords": [
    "graph",
    "graph-theory",
    "network",
    "node",
    "edge",
    "vertex",
    "link",
    "analysis",
    "visualisation",
    "visualization",
    "draw",
    "render",
    "biojs",
    "cytoscape"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "dist/cytoscape.umd.js",
  "scripts": {
    "lint": "eslint src",
    "build": "run-s build:*",
    "build:unmin": "cross-env NODE_ENV=development FILENAME=cytoscape.js MINIFY=false BABEL=true SOURCEMAPS=false webpack",
    "build:min": "cross-env NODE_ENV=development FILENAME=cytoscape.min.js MINIFY=true BABEL=true SOURCEMAPS=false webpack",
    "build:umd": "cross-env NODE_ENV=production FILENAME=cytoscape.umd.js MINIFY=false BABEL=true SOURCEMAPS=false webpack",
    "clean": "rimraf build/*",
    "copyright": "update license",
    "dist:copy": "cpy build/cytoscape.js build/cytoscape.min.js build/cytoscape.umd.js dist",
    "dist": "run-s build dist:*",
    "release": "run-s copyright dist docs",
    "postpublish": "run-s docs:push",
    "watch": "run-s watch:fast",
    "watch:sync": "livereload build",
    "watch:http": "http-server -s -c -1 -o",
    "watch:fast": "run-p watch:sync watch:http watch:build:fast",
    "watch:babel": "run-p watch:sync watch:http watch:build:babel",
    "watch:umd": "run-p watch:build:umd",
    "watch:build:fast": "cross-env NODE_ENV=development FILENAME=cytoscape.js MINIFY=false BABEL=false SOURCEMAPS=true webpack --progress --watch",
    "watch:build:babel": "cross-env NODE_ENV=development FILENAME=cytoscape.js MINIFY=false BABEL=true SOURCEMAPS=true webpack --progress --watch",
    "watch:build:umd": "cross-env NODE_ENV=production FILENAME=cytoscape.umd.js MINIFY=false BABEL=true SOURCEMAPS=false webpack --progress --watch",
    "test": "mocha",
    "test:build": "cross-env TEST_BUILD=true mocha",
    "test:travis": "run-s build:umd test:build",
    "sniper": "sniper .",
    "docs": "run-s docs:build docs:js",
    "docs:js": "cpy build/cytoscape.min.js documentation/js",
    "docs:build": "node documentation/docmaker.js",
    "docs:push": "gh-pages -d documentation",
    "benchmark": "run-s benchmark:single",
    "benchmark:download": "download https://raw.githubusercontent.com/cytoscape/cytoscape.js/master/dist/cytoscape.js --out build --filename cytoscape.benchmark.js",
    "benchmark:all:exec": "node benchmark/all",
    "benchmark:all": "run-s benchmark:download benchmark:all:exec",
    "benchmark:single:exec": "node benchmark/single",
    "benchmark:single": "run-s benchmark:download benchmark:single:exec"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.0",
    "chai": "^1.9.0",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.1.3",
    "download-cli": "^1.0.5",
    "eslint": "^4.16.0",
    "handlebars": "^4.0.5",
    "highlight.js": "^9.3.0",
    "http-server": "^0.11.1",
    "jsonlint": "^1.6.2",
    "livereload": "^0.6.3",
    "marked": "^0.3.5",
    "mocha": "^2.3.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "sniper": "^0.2.17",
    "update": "^0.7.4",
    "updater-license": "^1.0.0",
    "webpack": "^3.10.0"
  },
  "sniper": {
    "js": [
      "https://cdnjs.cloudflare.com/ajax/libs/fetch/2.0.3/fetch.min.js",
      "/build/cytoscape.js"
    ],
    "first": "images"
  },
  "dependencies": {
    "heap": "^0.2.6",
    "lodash.debounce": "^4.0.8"
  }
}
